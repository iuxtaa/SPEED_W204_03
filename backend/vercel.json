{
    "version": 2,
    "projectSettings": {
        "framework": "nodejs",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
    },
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
